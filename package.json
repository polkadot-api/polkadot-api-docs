    "polkadot-api": "^1.19.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "vocs": "1.0.13"
  },
  "devDependencies": {
    "@homer0/prettier-plugin-jsdoc": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "trailingComma": "all",
