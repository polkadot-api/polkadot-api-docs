{
  "name": "polkadot-api-docs",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vocs dev",
    "build": "vocs build",
    "preview": "vocs preview",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,md,mdx,css,cjs}\"",
    "prepare": "husky",
    "postinstall": "papi"
  },
  "dependencies": {
    "@polkadot-api/descriptors": "file:.papi/descriptors",
    "@radix-ui/react-tabs": "^1.1.13",
